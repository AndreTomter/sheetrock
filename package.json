{
  "name": "sheetrock",
  "version": "0.3.0",
  "description": "Quickly connect to, query, and lazy-load data from Google Spreadsheets.",
  "keywords": [
    "ecosystem:jquery",
    "jquery-plugin",
    "spreadsheet",
    "tables",
    "google",
    "googledocs",
    "ajax",
    "nodb"
  ],
  "homepage": "http://chriszarate.github.io/sheetrock/",
  "dependencies": {
    "request": "^2.53.0"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "jscs": "^1.11.3",
    "jshint": "^2.6.3",
    "node-qunit-phantomjs": "^1.2.0",
    "nodeunit": "^0.9.1",
    "uglify-js": "^2.4.17"
  },
  "browser": {
    "request": false
  },
  "main": "./src/sheetrock.js",
  "scripts": {
    "dist": "uglifyjs src/sheetrock.js --comments '/^!/' --source-map dist/sheetrock.min.js.map --prefix relative --output dist/sheetrock.min.js",
    "lint": "jshint src/*.js && jscs src/*.js",
    "test": "npm run lint && nodeunit test/nodeunit && node-qunit-phantomjs test/qunit/*.html",
    "pretest": "browserify test/qunit/src/browserify.js > test/qunit/build/browserify.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chriszarate/sheetrock"
  },
  "author": {
    "name": "Chris Zarate",
    "url": "http://chris.zarate.org"
  },
  "license": "MIT"
}
